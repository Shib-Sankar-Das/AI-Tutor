{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/api/test.py"
    },
    {
      "source": "/api/test/imports",
      "destination": "/api/test.py"
    },
    {
      "source": "/api/test/modules",
      "destination": "/api/test.py"
    },
    {
      "source": "/api/test/index",
      "destination": "/api/test.py"
    },
    {
      "source": "/api/chat",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/tts",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/ingest",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/memory/:path*",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/health",
      "destination": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/test.py": {
      "maxDuration": 10,
      "memory": 256
    },
    "api/minimal.py": {
      "maxDuration": 10,
      "memory": 256
    }
  }
}
